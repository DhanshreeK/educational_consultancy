<h1> otpVerifictions</h1>
<%= link_to 'New', new_otp_verification_path %>
<%= render 'layouts/messages'%> 
<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-primary">
      <div class="panel-heading clearfix">
        <h3 class="panel-title">Otp Verifictions</h3>
        <ul class="panel-tool-options"> 
          <li><a data-rel="collapse" href="#"><i class="icon-down-open"></i></a></li>
          <li><a data-rel="reload" href="#"><i class="icon-arrows-ccw"></i></a></li>
          <li><a data-rel="close" href="#"><i class="icon-cancel"></i></a></li>
        </ul>
      </div>
      <div class="panel-body">
        <div class="table-responsive">
          <%= link_to 'New otp_verification', new_otp_verification_path %>         
          <table class="table table-striped table-bordered table-hover dataTables-example" >
            <thead>
              <tr>
               <th>Contact No</th>
              
            
                <th></th>
             </tr>
           </thead>
           <tbody>
         
            <tr class="gradeX">
          <% @otp_verifications.each do |otp| %>
            <tr>
                <td><%= otp.contact_no %></td>
              
                <td><%= link_to 'Show', otp_verification_path(otp) %></td>
                 <td><%= link_to 'Destroy',otp_verification_path(otp),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
              
            </tr>
            <%end%>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
</div>