  <!-- Main container -->
    <div class="main-container gray-bg">
  
		<!-- Main header -->
		<div class="main-header row">
		  <div class="col-sm-6 col-xs-7">
		  
			<!-- User info -->
			<ul class="user-info pull-left">          
			  <li class="profile-info dropdown"><a data-toggle="dropdown" class="dropdown-toggle" href="#" aria-expanded="false"> <img width="44" class="img-circle avatar" alt=""  <% if current_user.role == 'SuperAdmin' || current_user.role == 'Employee' || (current_user.role == 'Center' && current_user.center.center_status == 'Active') %>
                <%= image_tag current_user.photo %>
               <% elsif  current_user.role == 'Center' %>
               <%= image_tag('man-3.jpg')%> 
                <% end %>
                <%= current_user.role%> <span class="caret"></span></a>
			  
				<!-- User action menu -->
				<ul class="dropdown-menu">
				   <% if current_user.role == 'SuperAdmin'%>
				  <li><a href="/users/1"><i class="icon-user"></i>My profile</a></li>
				  <li><a href="/general_settings/new"><i class="icon-cog"></i>Account settings</a></li>
				  <%end%>
				  <li><a href="#/"><i class="icon-mail"></i>Messages</a></li>
				  <li><a href="#"><i class="icon-clipboard"></i>Tasks</a></li>
				  <li class="divider"></li>
				  
				  <li><a data-method="delete" href="/users/sign_out"><i class="icon-logout"></i>Logout</a>
				  	
				  </li></ul>
		  </div>
 

