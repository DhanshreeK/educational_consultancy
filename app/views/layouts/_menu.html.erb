					<li><a href="/bulk_sms_settings/new"><span class="title">Bulk Sms Settings</span></a></li>
				</ul>
			</li>
		<%end%>


			<li class="has-sub"><a href="#"><i class="icon-list"></i><span class="title">Reports</span></a>
				<ul class="nav collapse">

